{
  
  "name": "p05",
  
  "version": "1.0.0",
  
  "homepage": "https://kitakitakita18.github.io/p05",
  
  "description": "マンション理事会管理システム",
  
  "main": "server/index.js",
  
  "scripts": {
  
    
  "dev": "concurrently \"npm run server\" \"npm run client\"",
  
    
  "start": "NODE_ENV=production node server/index.js",
  
    
  "stop": "./stop.sh",
  
    
  "server": "PORT=5105 nodemon server/index.js",
  
    
  "server:ts": "PORT=5105 ts-node server/app.ts",
  
    
  "server:full-ts": "PORT=5105 ts-node server/server.ts",
  
    
  "build:server": "cd server && tsc",
  
    
  "client": "cd client && PORT=3105 npm start",
  
    
  "build": "npm install --prefix client && npm run build --prefix client",
  
    
  "build:client": "cd client && npm run build",
  
    
  "test": "jest",
  
    
  "lint": "eslint . --ext .js,.ts,.tsx",
  
    
  "predeploy": "npm run build",
  
    
  "deploy": "gh-pages -d build",
  
    
  "typecheck": "cd client && npx tsc --noEmit"
  
  },
  
  "dependencies": {
  
    
  "axios": "^1.10.0",
  
    
  "bcryptjs": "^2.4.3",
  
    
  "cors": "^2.8.5",
  
    
  "dotenv": "^16.3.1",
  
    
  "express": "^4.18.2",
  
    
  "jsonwebtoken": "^9.0.2",
  
    
  "multer": "^1.4.5-lts.1",
  
    
  "nodemailer": "^6.9.8",
  
    
  "openai": "^5.9.0",
  
    
  "pdf-parse": "^1.1.1",
  
    
  "pg": "^8.11.3",
  
    
  "sqlite": "^5.1.1",
  
    
  "sqlite3": "^5.1.7",
  
    
  "tiktoken": "^1.0.21"
  
  },
  
  "devDependencies": {
  
    
  "@types/axios": "^0.9.36",
  
    
  "@types/bcryptjs": "^2.4.6",
  
    
  "@types/cors": "^2.8.17",
  
    
  "@types/express": "^4.17.23",
  
    
  "@types/jsonwebtoken": "^9.0.5",
  
    
  "@types/multer": "^1.4.11",
  
    
  "@types/node": "^20.19.7",
  
    
  "@types/nodemailer": "^6.4.14",
  
    
  "@types/pdf-parse": "^1.1.5",
  
    
  "@types/pg": "^8.10.9",
  
    
  "@typescript-eslint/eslint-plugin": "^6.15.0",
  
    
  "@typescript-eslint/parser": "^6.15.0",
  
    
  "concurrently": "^8.2.2",
  
    
  "eslint": "^8.56.0",
  
    
  "gh-pages": "^6.3.0",
  
    
  "jest": "^29.7.0",
  
    
  "nodemon": "^3.0.2",
  
    
  "ts-node": "^10.9.2",
  
    
  "typescript": "^5.8.3"
  
  },
  
  "keywords": [
  
    
  "mansion",
  
    
  "management",
  
    
  "system",
  
    
  "react",
  
    
  "node"
  
  ],
  
